{
  "name": "coffee-tmpl",
  "version": "1.2.1",
  "description": "javascript template engine. only browser.",
  "main": "lib/tmpl.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test/index.js",
    "report": "mocha test/index.js -r blanket -R html-cov > coverage.html",
    "doc": "jsdoc lib/tmpl.js",
    "dev": "babel lib/tmpl.js --out-file build/tmpl.js --watch",
    "build": "babel lib/tmpl.js --out-file build/tmpl.js"
  },
  "config": {
    "blanket": {
      "pattern": "build",
      "data-cover-never": "node_modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coffeedeveloper/coffee-tmpl.git"
  },
  "keywords": [
    "template"
  ],
  "author": "CoffeeDeveloper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coffeedeveloper/coffee-tmpl/issues"
  },
  "homepage": "https://github.com/coffeedeveloper/coffee-tmpl#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-plugin-syntax-export-extensions": "^6.3.13",
    "babel-plugin-transform-es2015-modules-umd": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "blanket": "^1.2.2",
    "gulp": "^3.9.0",
    "gulp-dev": "^0.3.0",
    "gulp-uglifyjs": "^0.6.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.3.4",
    "should": "^8.1.1"
  }
}
